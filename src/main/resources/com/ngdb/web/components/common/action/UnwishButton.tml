<span xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd" xmlns:p="tapestry:parameter">
	<t:if test="asButton">
		<t:actionLink class="btn btn-danger" t:id="unwish" context="article" t:mixins="jquery/tooltip" title="Remove from your wishlist">
			<i class="icon-fire icon-white" />
		</t:actionLink>
		<p:else>
			<t:actionLink t:id="unwishLink" context="article">
				<i class="icon-fire" /> Remove from your wishlist
			</t:actionLink>
		</p:else>
	</t:if>
</span>

